<?xml version="1.0" encoding="utf-8"?>
<com.appnext.nativeads.NativeAdView
    android:id="@id/advin_native"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <FrameLayout
        android:id="@id/fl_advin"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <androidx.cardview.widget.CardView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            card_view:cardBackgroundColor="#ffffffff"
            card_view:cardCornerRadius="4.0dip"
            cardUseCompatPadding="true"
            contentPadding="0.0dip"
            xmlns:card_view="http://schemas.android.com/apk/res-auto">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">
                <com.appnext.nativeads.MediaView
                    android:id="@id/ad_img_main"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" />
                <LinearLayout
                    android:orientation="horizontal"
                    android:padding="5.0dip"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:layout_gravity="center_vertical"
                        android:id="@id/ad_img"
                        android:visibility="gone"
                        android:layout_width="40.0dip"
                        android:layout_height="40.0dip"
                        android:layout_marginLeft="10.0dip"
                        android:layout_marginRight="5.0dip"
                        android:layout_marginStart="10.0dip"
                        android:layout_marginEnd="5.0dip" />
                    <LinearLayout
                        android:layout_gravity="center_vertical"
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5.0dip"
                        android:layout_marginRight="5.0dip">
                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                            <TextView
                                android:textSize="14.0dip"
                                android:textStyle="bold"
                                android:textColor="#ff000000"
                                android:layout_gravity="center_vertical"
                                android:id="@id/ad_tit"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="5.0dip"
                                android:layout_marginEnd="5.0dip" />
                            <ImageView
                                android:layout_gravity="center_vertical"
                                android:id="@id/rank1"
                                android:visibility="gone"
                                android:layout_width="14.0dip"
                                android:layout_height="14.0dip"
                                android:src="@drawable/favorito_p_2" />
                            <ImageView
                                android:layout_gravity="center_vertical"
                                android:id="@id/rank1_medio"
                                android:visibility="gone"
                                android:layout_width="14.0dip"
                                android:layout_height="14.0dip"
                                android:src="@drawable/favorito_p_2_medio"
                                android:rotationY="@integer/angle_rtl_180" />
                            <ImageView
                                android:layout_gravity="center_vertical"
                                android:id="@id/rank1_g"
                                android:visibility="gone"
                                android:layout_width="14.0dip"
                                android:layout_height="14.0dip"
                                android:src="@drawable/favorito_p_2_g" />
                            <ImageView
                                android:layout_gravity="center_vertical"
                                android:id="@id/rank2"
                                android:visibility="gone"
                                android:layout_width="14.0dip"
                                android:layout_height="14.0dip"
                                android:src="@drawable/favorito_p_2" />
                            <ImageView
                                android:layout_gravity="center_vertical"
                                android:id="@id/rank2_medio"
                                android:visibility="gone"
                                android:layout_width="14.0dip"
                                android:layout_height="14.0dip"
                                android:src="@drawable/favorito_p_2_medio"
                                android:rotationY="@integer/angle_rtl_180" />
                            <ImageView
                                android:layout_gravity="center_vertical"
                                android:id="@id/rank2_g"
                                android:visibility="gone"
                                android:layout_width="14.0dip"
                                android:layout_height="14.0dip"
                                android:src="@drawable/favorito_p_2_g" />
                            <ImageView
                                android:layout_gravity="center_vertical"
                                android:id="@id/rank3"
                                android:visibility="gone"
                                android:layout_width="14.0dip"
                                android:layout_height="14.0dip"
                                android:src="@drawable/favorito_p_2" />
                            <ImageView
                                android:layout_gravity="center_vertical"
                                android:id="@id/rank3_medio"
                                android:visibility="gone"
                                android:layout_width="14.0dip"
                                android:layout_height="14.0dip"
                                android:src="@drawable/favorito_p_2_medio"
                                android:rotationY="@integer/angle_rtl_180" />
                            <ImageView
                                android:layout_gravity="center_vertical"
                                android:id="@id/rank3_g"
                                android:visibility="gone"
                                android:layout_width="14.0dip"
                                android:layout_height="14.0dip"
                                android:src="@drawable/favorito_p_2_g" />
                            <ImageView
                                android:layout_gravity="center_vertical"
                                android:id="@id/rank4"
                                android:visibility="gone"
                                android:layout_width="14.0dip"
                                android:layout_height="14.0dip"
                                android:src="@drawable/favorito_p_2" />
                            <ImageView
                                android:layout_gravity="center_vertical"
                                android:id="@id/rank4_medio"
                                android:visibility="gone"
                                android:layout_width="14.0dip"
                                android:layout_height="14.0dip"
                                android:src="@drawable/favorito_p_2_medio"
                                android:rotationY="@integer/angle_rtl_180" />
                            <ImageView
                                android:layout_gravity="center_vertical"
                                android:id="@id/rank4_g"
                                android:visibility="gone"
                                android:layout_width="14.0dip"
                                android:layout_height="14.0dip"
                                android:src="@drawable/favorito_p_2_g" />
                            <ImageView
                                android:layout_gravity="center_vertical"
                                android:id="@id/rank5"
                                android:visibility="gone"
                                android:layout_width="14.0dip"
                                android:layout_height="14.0dip"
                                android:src="@drawable/favorito_p_2" />
                            <ImageView
                                android:layout_gravity="center_vertical"
                                android:id="@id/rank5_medio"
                                android:visibility="gone"
                                android:layout_width="14.0dip"
                                android:layout_height="14.0dip"
                                android:src="@drawable/favorito_p_2_medio"
                                android:rotationY="@integer/angle_rtl_180" />
                            <ImageView
                                android:layout_gravity="center_vertical"
                                android:id="@id/rank5_g"
                                android:visibility="gone"
                                android:layout_width="14.0dip"
                                android:layout_height="14.0dip"
                                android:src="@drawable/favorito_p_2_g" />
                            <ImageView
                                android:layout_gravity="center_vertical"
                                android:id="@id/appnext_logo"
                                android:layout_width="wrap_content"
                                android:layout_height="14.0dip"
                                android:layout_marginLeft="10.0dip"
                                android:src="@drawable/info"
                                android:adjustViewBounds="true"
                                android:layout_marginStart="10.0dip" />
                        </LinearLayout>
                        <TextView
                            android:textSize="13.0dip"
                            android:textColor="#ff000000"
                            android:id="@id/ad_descr"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:maxLines="2" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </androidx.cardview.widget.CardView>
    </FrameLayout>
</com.appnext.nativeads.NativeAdView>