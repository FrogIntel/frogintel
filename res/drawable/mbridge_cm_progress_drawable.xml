<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@android:id/background">
        <shape>
            <corners
                android:radius="25.0dip" />
            <gradient
                android:startColor="#ff8fc31f"
                android:endColor="#ff8fc31f"
                android:angle="0.0" />
        </shape>
    </item>
    <item
        android:id="@android:id/progress">
        <clip>
            <shape>
                <corners
                    android:radius="25.0dip" />
                <gradient
                    android:startColor="#ff73a210"
                    android:endColor="#ff73a210"
                    android:angle="0.0" />
            </shape>
        </clip>
    </item>
</layer-list>